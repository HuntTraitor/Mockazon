  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 14
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  }

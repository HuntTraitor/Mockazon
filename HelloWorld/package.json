    "graphql-request": "*",
    "graphql-yoga": "*",
    "jsonwebtoken": "*",
    "next": "*",
    "pg": "*",
    "react": "*",
    "react-dom": "*",
    "reflect-metadata": "*",
    "type-graphql": "*"
  },
  "devDependencies": {
    "@jest/globals": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "@types/glob": "*",
    "@types/jest": "*",
    "@types/jsonwebtoken": "*",
    "@types/node": "*",
    "@types/pg": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/supertest": "*",

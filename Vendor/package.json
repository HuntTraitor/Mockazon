{
  "name": "mockazon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start -p 3003",
    "lint": "next lint",
    "fix": "prettier --write ./**/*.{ts,tsx} && next lint --fix",
    "test-frontend": "jest test/views --runInBand --detectOpenHandles --forceExit",
    "test-backend": "jest test/api --runInBand --detectOpenHandles --forceExit",
    "test-e2e": "cd test/e2e && jest --testTimeout=10000 --runInBand --verbose --forceExit --detectOpenHandles",
    "test:silent": "npm test -- --silent",
    "test:watch": "npm test -- --watchAll"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.5",
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "class-validator": "*",
    "cors": "^2.8.5",
    "graphql": "*",
    "graphql-request": "*",
    "graphql-yoga": "*",
    "jsonwebtoken": "*",
    "next": "*",
    "pg": "*",
    "react": "*",
    "react-dom": "*",
    "reflect-metadata": "*",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.1",
    "type-graphql": "^2.0.0-beta.6"
  },
  "devDependencies": {
    "@jest/globals": "*",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/glob": "*",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "*",
    "@types/node": "*",
    "@types/pg": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/supertest": "*",
    "bestzip": "*",
    "dotenv": "^16.4.5",
    "eslint": "*",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "*",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-typescript": "*",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "*",
    "msw": "*",
    "next-router-mock": "*",
    "prettier": "^3.2.5",
    "puppeteer": "^22.7.1",
    "supertest": "*",
    "ts-jest": "^29.1.2",
    "ts-node": "*",
    "ts-postgres": "*",
    "typescript": "*"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "typescript"
